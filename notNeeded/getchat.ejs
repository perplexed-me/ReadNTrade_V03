<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Chat History</title>
</head>
<body>
    <h1>Chat History</h1>
    <div id="chatContainer">
        <% chatData.forEach(message => { %>
            <div><%= message.text %> - <%= message.msz_time %></div>
        <% }); %>
    </div>
</body>
</html>
